#!/bin/bash
echo "$(( $(echo "$WATER" | awk 'BEGIN{map["e"]=0;map["w"]=1;map["a"]=2;map["t"]=3;map["r"]=4} {sum=0; for(i=1;i<=length($0);i++){c=substr($0,i,1); sum=sum*5+map[c]}; print sum}') + $(echo "$STIR" | awk 'BEGIN{map["t"]=0;map["i"]=1;map["."]=2;map["r"]=3} {sum=0; for(i=1;i<=length($0);i++){c=substr($0,i,1); sum=sum*4+map[c]}; print sum}') ))" | awk '{b="shtbeolhc"; n=$1; r=""; while(n>0){r=substr(b,(n%8)+1,1)r; n=int(n/8)} print r}'
